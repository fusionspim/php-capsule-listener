{
  "name": "fusionspim/php-capsule-listener",
  "description": "Simple debugging tool for usage with Laravel's Eloquent ORM",
  "keywords": ["capsule", "debug", "eloquent", "laravel", "orm"],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "illuminate/database": "^5.8 || ^6.5",
    "illuminate/events": "^5.8 || ^6.5",
    "symfony/var-dumper": "~4.0 || ~5.0",
    "ext-json": "^1.6",
    "ext-mbstring": "^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "8.4.*",
    "ext-sqlite3": "^7.2",
    "fusionspim/php-cs-fixer-config": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "FusionsPim\\PhpCapsuleListener\\": "src/",
      "FusionsPim\\Tests\\PhpCapsuleListener\\": "tests/"
    },
    "files": ["src/functions.php"]
  }
}
